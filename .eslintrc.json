{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
      "ecmaVersion": 2017
  },
  "ignorePatterns": ["node_modules/"],
  "env": {
    "es6": true,
    "browser": false,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}